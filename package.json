{
  "name": "github-stupidity",
  "version": "1.0.0",
  "description": "Analyze the stupidity of GitHub repositories",
  "scripts": {
    "dev": "npm run build && node build/server.js",
    "test": "jest --runInBand",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "tsc && node build/server.js",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "prettier": "prettier --write \"src/**/*.ts{,?}\"",
    "prettier-diff": "prettier --list-different \"src/**/*.tsx{,?}\""
  },
  "author": "James Wang",
  "license": "MIT",
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "axios": "^0.18.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "next": "^8.0.3",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "reactstrap": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/next": "^8.0.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/reactstrap": "^7.1.3",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "typescript": "^3.3.4000"
  }
}
